{
	"cluster": {
		"nodeId": ""
	},
	"db": {
		"embedded": false,
		"host": "localhost",
		"port": 5432,
		"name": "__DB_NAME__",
		"user": "__DB_USER__",
		"pass": "__DB_PWD__",
		"ssl": false,
		"sslSkipVerify": false,
		"connsMax": 0,
