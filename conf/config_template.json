		"temp": "data/temp/",
		"transfer": "data/transfer"
	},
	"portable": false,
	"web": {
		"cert": "/etc/yunohost/certs/__DOMAIN__/crt.pem",
		"key": "/etc/yunohost/certs/__DOMAIN__/key.pem",
